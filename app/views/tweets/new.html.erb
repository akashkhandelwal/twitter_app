<%= form_for Tweet.new do |f| %>
	<%= render 'shared/error_messages', object: f.object %>

  <p>
  	<%= f.label :body %><br />
    <%= f.text_area :body, :cols => 50, :rows => 3 %>
  </p>

  <p>
  	<%= f.check_box :private %>
  	<%= f.label :private %>
  </p>

  <p class="button"><%= f.submit %></p>

<% end %>